{
  "name": "ennacx/cake-sentry",
  "description": "Sentry plugin for CakePHP",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "cakephp/cakephp": "^5.0",
    "sentry/sentry": "^4.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4"
  },
  "autoload": {
    "psr-4": {
      "Ennacx\\CakeSentry\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ennacx\\CakeSentry\\Test\\": "tests/",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
